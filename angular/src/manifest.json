{
  "manifest_version": 3,
  "name": "test1",
  "short_name": "test1",
  "version": "1.0.0",
  "description": "Generated with @larscom/ng-chrome-extension",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "scripting",
    "activeTab",
    "identity"
  ],
  "oauth2": {
    "client_id": "131178090127-1ve3a1fakntna7fh0vsf9f0rcjku58oi.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "host_permissions": ["https://www.linkedin.com/*", "https://api.notion.com/"],
  "action": {
    "default_popup": "index.html?#/popup",
    "default_title": "Angular Chrome Extension"
  },
  "options_page": "index.html?#/options",
  "chrome_url_overrides": {
    "newtab": "index.html?#/tab"
  },
  "content_scripts": [
    {
      "js": ["./chrome/src/contentPage.js"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ],
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+0",
        "mac": "Command+Shift+0"
      },
      "description": "Toggle the helpcenter screen"
    }
  },
  "background": {
    "service_worker": "./chrome/src/background.js",
    "type": "module"
  }
}
