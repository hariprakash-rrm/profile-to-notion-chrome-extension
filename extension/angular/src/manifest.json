{
  "manifest_version": 3,
  "name": "ProfileToNotion",
  "short_name": "pTn",
  "version": "1.0.0",
  "description": "Generated with @larscom/ng-chrome-extension",
  "icons": {
    "48": "assets/logo.jpeg",
    "96": "assets/logo.jpeg",
    "144": "assets/logo.jpeg",
    "192": "assets/logo.jpeg"
  },

  
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "activeTab",
    "identity",
    "notifications"
  ],
  
  "oauth2": {
    "client_id": "131178090127-p9t30b3cgp260it21bohtgf334tvf59a.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "action": {
    "default_popup": "index.html?#/popup",
    "default_title": "Angular Chrome Extension",
    "default_icon": {
      "16": "assets/logo.jpeg",
      "32": "assets/logo.jpeg"
    }
  },
  "options_page": "index.html",
  
  "content_scripts": [
    {
      "js": ["./chrome/src/contentPage.js"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ],
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+4",
        "mac": "Command+Shift+4"
      },
      "description": "Toggle the helpcenter screen"
    }
  },
  "background": {
    "service_worker": "./chrome/src/background.js",
    "type": "module"
  }
 
}
